{
    "name": "fitenson/test-lab",
    "description": "Test Case Library for Yii2",
    "type": "library",
    "require": {
        "yiisoft/yii2": "~2.0.0",
        "codeception/codeception": "^4.0",
        "codeception/module-yii2": "^1.0.0",
        "phpunit/phpunit": "^8.5",
        "fakerphp/faker": "^1.13",
        "yiisoft/yii2-faker": "^2.0.0",
        "codeception/module-phpbrowser": "^1.0.0",
        "codeception/module-asserts": "^1.0.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Fitenson\\TestLab\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Fitenson"
        }
    ],
    "minimum-stability": "stable",
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}